            Background="Transparent"
            Orientation="Vertical">
            <DockPanel
                Margin="40, 20, 0, 0">
                <Button Command="{Binding OpenSideMenuCommand}" Style="{StaticResource StyleTopBarButton}" Panel.ZIndex="20">
                    <Grid x:Name="grid" Width="30" RenderOptions.BitmapScalingMode="HighQuality" SnapsToDevicePixels="False">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Line Grid.Row="0" Style="{StaticResource StyleTopBarButtonMenuStripes}"/>
                        <Line Grid.Row="2" Style="{StaticResource StyleTopBarButtonMenuStripes}"/>
                        <Line Grid.Row="4" Style="{StaticResource StyleTopBarButtonMenuStripes}"/>
                    </Grid>
                </Button>
            </DockPanel>
            <Grid>
                <local:SideMenu Visibility="{Binding SideMenuVisibility}" Grid.Column="0" Grid.Row="0" Panel.ZIndex="40" HorizontalAlignment="Left" Margin="25, 30, 0, 0"/>
                <local:NewEmailGame Visibility="{Binding NewEmailGameVisibility}" Grid.Column="0" Grid.Row="0" Panel.ZIndex="40" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <local:Settings Visibility="{Binding SettingsVisibility}" Grid.Column="0" Grid.Row="0" Panel.ZIndex="40" HorizontalAlignment="Center" VerticalAlignment="Center"/>
