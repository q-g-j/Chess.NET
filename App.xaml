<Application x:Class="ChessDotNET.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ChessDotNET.Views"
             StartupUri="Views/MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/ResourceDictionaries/Fonts.xaml" />
                <ResourceDictionary Source="/ResourceDictionaries/ChessPieceImages.xaml" />
                <ResourceDictionary Source="/ResourceDictionaries/MainWindowStyles.xaml" />
                <ResourceDictionary Source="/ResourceDictionaries/SettingsStyles.xaml" />
                <ResourceDictionary Source="/ResourceDictionaries/SideMenuStyles.xaml" />
                <ResourceDictionary Source="/ResourceDictionaries/NewEmailGameStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
